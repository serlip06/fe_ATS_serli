{
  "extends": [
    "development"
  ],
  "hints": {
    "axe/forms": [
      "default",
      {
        "label": "off",
        "select-name": "off"
      }
    ],
    "axe/name-role-value": [
      "default",
      {
        "button-name": "off"
      }
    ],
    "disown-opener": "off",
    "axe/text-alternatives": [
      "default",
      {
        "image-alt": "off",
        "document-title": "off"
      }
    ],
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "-webkit-text-size-adjust"
        ]
      }
    ],
    "axe/language": [
      "default",
      {
        "html-has-lang": "off"
      }
    ],
    "meta-charset-utf-8": "off",
    "meta-viewport": "off"
  }
}